<script>
    $(document).ready(() => {
        var editor;
        ClassicEditor
            .create(document.querySelector('#editor'))
            .then(neweditor => {
                console.log("Editor catched!")
                neweditor.model.document.on('change:data', () => {
                    console.log(neweditor.getData());
                });
                editor = neweditor;
            })
            .catch(error => {
                console.error(error);
            });
    })

</script>