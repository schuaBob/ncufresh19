<!DOCTYPE html>
<html>

<head>
    <title><%= title %></title>
    <% include ../layouts/head %>
    <% include ../layouts/script %>
    <link rel='stylesheet' href='/documents/style.css' />
</head>

<body class="other_bg">
    <% include ../layouts/header %>
    <main id="main">
        <% /*編輯框*/ %>
        <% include edit %>
        <% /*研究生*/%>
        <div class="graduate">
                <div class="nav">
                        <div class="return_space"><a href="/documents"><img src="/documents/return.png" class="return"></a></div>
                        <div class="sign">
                                <p class="p1">FRESH</p>
                                <p class="p2">新生週</p>
                        </div>
                </div>

                <div class="gra_bg">
                        <div class="sidenav">
                                <a href="/documents/graduate_must"><img src="/documents/入學須知_原.png" class="icon icon1"></a>
                                <a href="/documents/graduate_fresh"><img src="/documents/新生週.png" class="icon"></a>                 
                        </div>
                        <div class="must" id="must">
                                <div class="sidenav2">
                                        <p class="option page_now" id="29">＞&emsp;社團博覽會</p>
                                        <p class="option" id="30">＞&emsp;新生健康檢查</p>
                                </div>
                        </div>  
                </div>
                
                <% if (data === null){ %>
                        <div class="box">
                            <div class="title" >
                                <p id="show_title"></p>
                            </div>
                            <div class="content" >
                                
                            </div>
                        </div>
                    <% }else{ %>
                        <div class="box">
                            <div class="title" >
                                <p id="show_title"><%= data.title %></p>
                            </div>
                            <div class="content" >
                                <%- data.content %>
                            </div>
                        </div>
                    <% } %>

        </div>
    </main>
    <% include ../layouts/js %>
    <% include js %>
</body>

</html>