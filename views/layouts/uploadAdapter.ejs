<script type="text/javascript" src="/js/ckeditor.js"></script>
<script>
    class ckuploadAdapter {
        constructor(loader) {
            this.loader = loader;
        }
        upload() {
            /*resolved by a successful upload with an object containing information about the uploaded file,
            rejected because of an error, in which case nothing is inserted into the content.*/
            return new Promise((resolve, reject) => {
               resolve(this.loader);
            })
        }
        abort() {
            console.log("abort")
        }
    }
    function cusUploadAdapterPlugin(editor) {
        editor.plugins.get('FileRepository').createUploadAdapter = (loader) => {
            return new ckuploadAdapter(loader)
        }
    }
    




</script>