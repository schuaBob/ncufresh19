 $(document).ready(function() {
    var url = new URL(window.location.href);
    url = url.href.split('/');
     function navi(i) {
        $(".navSubBtn").eq(i).hover(
              function() {
                $(this).attr("src", "naviAfter" + i + ".png")
              },
            function() {
                if (i != match_num[url[4]])
                    $(this).attr("src", "naviBefore" + i + ".png")
            })
     }
     for(i=0; i<4; i++) {
            navi(i);
     }
 });

